<grammar
  xmlns="http://www.w3.org/2001/06/grammar"
  tag-format="semantics/1.0-literals"
  version="1.0"
  root="rootRule"
  xml:lang="en-US"
  >

  <rule id="rootRule">
    <one-of>

      <item>
        <tag>PLAY</tag>
        <one-of>
          <item> play </item>
          <item> test </item>
          <item> try </item>
          <item> visualize </item>
          <item> visualizer </item>
        </one-of>
      </item>

      <item>
        <tag>CLOSE_VISUALIZER</tag>
        <one-of>
          <item> close </item>
          <item> close visualizer </item>
        </one-of>
      </item>

    </one-of>
  </rule>
</grammar>